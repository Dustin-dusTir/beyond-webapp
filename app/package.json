		"@smui/common": "^6.1.4",
		"@smui/dialog": "^6.1.4",
		"@smui/linear-progress": "^6.1.4",
		"@smui/snackbar": "^6.1.4",
		"@smui/tab": "^6.1.4",
		"@smui/tab-bar": "^6.1.4",
		"@smui/tooltip": "^6.1.4",
		"@sveltejs/adapter-auto": "next",
		"@sveltejs/adapter-static": "^1.0.0-next.38",
		"@sveltejs/kit": "next",
		"@typescript-eslint/eslint-plugin": "^5.27.0",
